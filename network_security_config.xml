<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>

    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">192.168.2.131:80</domain> <!--Ip address-->
        <domain includeSubdomains="true">192.168.2.131:80/api/barcode</domain> <!--Ip address-->
    </domain-config>
</network-security-config>
